   "source": [
    "# 결측치 대체하기"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "df.dropna() # 결측치 존재하는 행 삭제\n",
    "df.dropna(how= 'all') # 모든 변수가 결측치로만 존재하는 행 삭제\n",
    "\n",
    "df['col1'].replace(np.nan, df['col1'].mean()) # 결측치를 각 변수의 평균값으로 대체\n",
    "\n",
    "df.interpolate() # 선형 함수 기반 앞뒤 관측치 활용 대체\n",
    "\n",
    "df['col1'].fillna('값')"
   ]
  }
